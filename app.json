{
  "expo": {
    "name": "SoundFit",
    "slug": "EmodyApp",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/emody.png",
    "scheme": "emodyapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "buildNumber": "1",
      "supportsTablet": true,
      "bundleIdentifier": "com.gkapps713.emodyapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryAddUsageDescription": "EmodyApp needs access to your photo library to save generated videos."
      }
    },
    "android": {
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/emody.png",
        "backgroundColor": "#FDE4F2"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gkapps713.emodyapp",
      "targetSdkVersion": 33,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-audio",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow EmodyApp to access your photos and videos.",
          "savePhotosPermission": "Allow EmodyApp to save generated videos to your device.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e6e28ce4-9744-489a-8a43-c68b3e2b8089"
      }
    }
  }
}
